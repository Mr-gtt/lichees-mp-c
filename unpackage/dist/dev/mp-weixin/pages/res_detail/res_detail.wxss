scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.scroll-view_H{
	
  white-space: nowrap;
  overflow: hidden;
}
.scroll-view-item{
  width: 240rpx;
  height: 160rpx;
  display: inline-block;
  white-space: normal;
  /* display: flex; */
  /* display: inline; */
  margin-left: 20rpx;
  
  background-color: #FFF;
  border-radius: 20rpx;
}
.scroll-view-item .card1{
	color: #FFFFFF;
	background-repeat:no-repeat;
	/* background-size: cover; */
	background-image:-webkit-linear-gradient(top left, rgba(113, 164, 214, 0.9), rgba(5, 130, 232, 0.9)), 
					url("data:image/svg+xml;base64,");
	background-image:linear-gradient(to bottom right, rgba(113, 164, 214, 0.9), rgba(5, 130, 232, 0.9)), 
					url("data:image/svg+xml;base64,");
}
.scroll-view-item .card2{
	color: #FFFFFF;
	font-size: 1.5em;
	background-repeat:no-repeat;
	background-size: cover;
	background-image:-webkit-linear-gradient(top left, rgba(201, 104, 227, 0.9), rgba(133, 11, 174, 0.9)), 
					url("data:image/svg+xml;base64,");
	background-image:linear-gradient(to bottom right, rgba(201, 104, 227, 0.9), rgba(133, 11, 174, 0.9)), 
					url("data:image/svg+xml;base64,");
}
.scroll-view-item .card3{
	color: #FFFFFF;
	font-size: 1.5em;
	background-repeat:no-repeat;
	background-size: cover;
	background-image:-webkit-linear-gradient(top left, rgba(231, 118, 71, 0.9), rgba(214, 41, 12, 0.9)), 
					url("data:image/svg+xml;base64,");
	background-image:linear-gradient(to bottom right, rgba(231, 118, 71, 0.9), rgba(214, 41, 12, 0.9)), 
					url("data:image/svg+xml;base64,");
}
.scroll-view-item .card4{
	color: #FFFFFF;
	font-size: 3em;
	background-repeat:no-repeat;
	background-size: cover;
	background-image:-webkit-linear-gradient(top left, rgba(83, 214, 216, 0.9), rgba(8, 156, 192, 0.9)),
					url("data:image/svg+xml;base64,");
	background-image:linear-gradient(to bottom right, rgba(83, 214, 216, 0.9), rgba(8, 156, 192, 0.9)),
