
.releaseRes{
}
.releaseRes .mainInfo {
	background-color: #FFFFFF;

	border-radius: 30rpx;
	margin: 15rpx 20rpx;
	padding-top: 15rpx;
	padding-bottom: 15rpx;
}
.releaseRes .mainInfo .mainInfoTitle{
	border-left: 10rpx solid #1B97F5;
	margin: 3% 5%;
	padding-left: 20rpx;
}
.releaseRes .personInfo .mainInfoTitle {
	border-left: 10rpx solid #d54ebf;
}
.releaseRes .mainInfo .resLayout{
	padding: 3% 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	white-space: nowrap;
}
.releaseRes .mainInfo .resInput{
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
}
.releaseRes .mainInfo .resHelper {
	color: #5B5B5B;
	/* margin-right: 30upx; */
	width: 20%;
}
.divider{
	width: 100%;
	border-bottom: 1rpx solid #eae9ee; 
	margin-top: 5rpx; 
	margin-bottom: 5rpx;
}
.releaseRes .resBtnGroup{
	
	
	margin: 15rpx 20rpx;
	margin-top: 100rpx;
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: flex;
	-webkit-align-content: center;
	        align-content: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.releaseRes .resBtnGroup .resBtn1 {
	background-color: #797979;
	margin-left: 10rpx;
	margin-right: 10rpx;
	padding: 10rpx 20rpx;
	border-radius: 10rpx;
}
.releaseRes .resBtnGroup .resBtn2 {
	
	background-color: #0091ff;
	margin-left: 10rpx;
	margin-right: 10rpx;
	padding: 10rpx 20rpx;
	border-radius: 10rpx;
}
.releaseRes .resBtnGroup .resBtnText{
	color: #FFF;
	font-size: 1.2em;
	-webkit-align-self: center;
	        align-self: center;
}
	

