.cardView{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;border-radius:30rpx;margin-bottom:.5em;padding:30rpx 30rpx 15rpx 30rpx;background-color:#fff}.cardView .card .mainInfo .cardTitle{font-size:1.3em;margin-bottom:5rpx;height:1.3em}.cardView .card{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-align-self:stretch;align-self:stretch}.cardView .card  .colorBar{background-color:#007aff;margin-top:13rpx;margin-bottom:13rpx;width:12rpx;border-radius:8rpx;margin-right:30rpx}.cardView .card .classTime{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between;margin-right:30rpx}.cardView .card .classTime text{font-weight:700}.cardView .card .mainInfo{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;width:85%}.cardView .card .mainInfo .cardText location{font-size:1em;padding:0;margin:0;position:relative;top:-5rpx}.cardView .memberList{display:-webkit-box;display:-webkit-flex;display:flex}.cardView .memberList .member{background-color:#d9d9df;height:60rpx;width:60rpx;border-radius:30rpx;margin-right:-30rpx;z-index:1;box-shadow:3rpx 3rpx 10rpx #d0d0d0;border:1rpx solid #c3c3c3}.nowrap{white-space:nowrap;text-overflow:ellipsis;overflow:hidden}