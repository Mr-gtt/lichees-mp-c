.tabbar{z-index:1000;background-color:#fff;position:fixed;right:5%;left:5%;bottom:35rpx;height:100rpx;border-radius:50rpx;box-shadow:0 0 5px #c8c8c8;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;padding:3rpx}.tabbar .tab-item{width:30%;height:40rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;text-align:center}.tabbar .tab-item .icon{width:40rpx;height:40rpx;padding:5rpx}.tabbar .tab-item .text{font-size:25rpx;width:100%}.tabbar .tab-item .click{-webkit-animation:scaleDraw .5s ease-in-out;animation:scaleDraw .5s ease-in-out}.tabbar .tab-item .activedTabText{color:#007aff}@-webkit-keyframes scaleDraw{0%{-webkit-transform:scale(.8);transform:scale(.8)}50%{-webkit-transform:scale(1.1);transform:scale(1.1)}75%{-webkit-transform:scale(1);transform:scale(1)}}@keyframes scaleDraw{0%{-webkit-transform:scale(.8);transform:scale(.8)}50%{-webkit-transform:scale(1.1);transform:scale(1.1)}75%{-webkit-transform:scale(1);transform:scale(1)}}